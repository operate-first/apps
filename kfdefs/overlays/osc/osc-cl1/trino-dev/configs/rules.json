{
    "catalogs": [
        {
            "group": "admins",
            "allow": "all"
        },
        {
            "catalog": ".*",
            "allow": "read-only"
        }
    ],
    "schemas": [
        {
            "group": "admins",
            "owner": true
        },
        {
            "catalog": ".*",
            "schema": ".*",
            "owner": false
        }
    ],
    "tables": [
        {
            "group": "admins",
            "privileges": [
                "SELECT",
                "INSERT",
                "DELETE",
                "OWNERSHIP"
            ]
        },
        {
            "catalog": ".*",
            "schema": ".*",
            "table": ".*",
            "privileges": [
                "SELECT"
            ]
        }
    ]
}
